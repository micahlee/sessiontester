* {
	margin: 0;
	padding: 0;
}

body,html {
	margin: 0;
	padding: 0;
	background: #ECECEC;
	color: #333333;
	text-align: center;
}
blockquote {
	margin: 0px 40px 10px 40px;
	padding: 5px;
	background-color: #E5E5E5;
	border: solid 1px #CCCCCC;
}

#page {
	margin: 0 auto;
	padding: 15px 5px 10px 5px;
	width: 750px;
	background: #FFFFFF;
	color: #333333;
	text-align: center;
}

#header {
	width: 740px;
	margin: 0px;
	padding: 0px;
	white-space: nowrap;
	text-align: center;
}

.toplogo {
	border: 0;
	float: left;
	margin: 0px;
	padding: 0px;
}

#topmenu {
	float: right;
	margin: 0px;
	padding: 0px;
	width: 560px;
	background: #E5E5E5;
	color: #333333;
	text-align: left;
}

.topmenubutton {
	margin: 1px 1px 0px 1px;
	padding: 3px 5px 3px 5px;
	float: left;
	border-right: 1px dotted #CCCCCC;
	display: inline;
	list-style: none;
	font: 9px Verdana, Arial, sans-serif;
	text-align: center;
}

.topmenubuttonNo {
	margin: 1px 1px 0px 3px;
	padding: 3px 5px 3px 5px;
	float: left;
	display: inline;
	list-style: none;
	font: 9px Verdana, Arial, sans-serif;
	text-align: center;
}

a.topmenubutton:link, a.topmenubutton:visited {
	border-right: none;
	padding: 3px;
	background: #E5E5E5;
	color: #333333;
	text-decoration: none;
	font-weight: normal;
}

a.topmenubuttonNo:link, a.topmenubuttonNo:visited {
	padding: 3px;
	background: #E5E5E5;
	color: #333333;
	text-decoration: none;
	font-weight: normal;
}

a.topmenubutton:active, a.topmenubutton:hover {
	border-right: none;
	background: #F4F4F4;
	color: #76A544;
	font-weight: normal;
}

a.topmenubuttonNo:active, a.topmenubuttonNo:hover {
	background: #F4F4F4;
	color: #76A544;
	font-weight: normal;
}

#title {
	margin: 0px;
	padding: 0px;
	float: right;
	width: 560px;
	text-align: left;
}

#title h1 {
	margin: 0px;
	padding: 15px;
	color: #FFFFFF;
	background: #669933;  
	font: 24px Verdana, Arial, sans-serif;
}

#leftcolumn {
	float: left;
	clear: both;
	padding: 0px;
	margin: 10px 0px 0px 5px;
	width: 160px;
	text-align: left;
}

#leftmenu {
	margin: 0px;
	padding: 0px 0px 10px 0px;
	background: #C1D7AB;
	color: #41651A;
	font: 11px Verdana, Arial, sans-serif;
}

#leftmenu h1{
	margin: 0px;
	padding: 5px 10px 5px 10px;
	background: #669933;
	color: #FFFFFF;
	font: 18px Verdana, Arial, sans-serif;
	text-align: center;
}

#leftmenu p{
	margin: 0px;
	padding: 10px;
}

#leftmenu ul{
	margin: 8px 10px 5px 10px;
}

#leftmenu li {
	border-bottom: 1px solid #FFFFFF;
	margin: 0px;
	padding: 0px 0px 2px 0px;
	list-style: none;
	color: #41651A;
	background: #C1D7AB;
}

a.menuul, a.menuul:link, a.menuul:visited {
	color: #41651A;
	background: #C1D7AB;
	text-decoration: none;
	font-weight: normal;
}

a.menuul:active, a.menuul:hover {
	background: #FFFFFF;
	color: #000000;
	text-decoration: none;
	font-weight: normal;
}

#sitedesign {
	margin: 20px 0px 20px 0px;
	padding: 5px;
	background: #FFFFFF;
	color: #666666;
}

#sitedesign p{
	border: 0;
	font: 10px Verdana, Arial, sans-serif;
	text-decoration: none;
}

#sitedesign img{
	border: none;
}

#sitedesign a, a:link, a:hover, a:active, a:visited {
	border: 0;
	text-decoration: none;
	font-weight: normal;
}

#leftlist {
	border-bottom: 1px solid #76A544;
	border-left: 1px solid #76A544;
	border-right: 1px solid #76A544;
	margin: 20px 0px 0px 0px;
	padding: 0px;
	text-align: left;
	font: 11px/13px Verdana, Arial, sans-serif;
}
#leftlist h2 {
	margin: 0px;
	padding: 5px 10px 5px 10px;
	background: #76A544;
	color: #FFFFFF;
	font: 18px Verdana, Arial, sans-serif;
	text-align: center;
}
#leftlist h3 {
	border: none;
	margin: 0px;
	padding: 10px 10px 0px 10px;
	font: bold 12px Verdana, Arial, sans-serif;
}
#leftlist ul {
	margin: 5px 15px 10px 25px;
	padding: 0px;
	list-style: none;
}
#leftlist li {
	margin: 5px 0px 5px 0px;
	padding: 0px;
	text-indent: -10px;
	text-align: left;
}
#leftlist p {
	margin: 5px 10px 5px 10px;
	padding: 0px;
}
#leftlist a, a:link, a:visited #content a:link, #content a:visited {
	color: #41651A;
	text-decoration: none;
	font-weight:normal;
}
#leftlist a:active, a:hover, #content a:active, #content a:hover {
	color: #336699;
	text-decoration: underline;
	background: white;
	font-weight: normal;
}
#content{
	float: left;
	margin: 0px 10px 25px 20px;
	padding: 0px;
	width: 540px;
	font: 12px/18px Verdana, Arial, sans-serif;
	text-align: left;
}

#content h1 {
	border-bottom: 2px dotted #CCCCCC;
	margin: 5px 0px 10px 0px;
	padding: 0px;
	color: #000000;
	background: #FFFFFF;
	font: 20px Verdana, Arial, sans-serif;
}

#content h2 {
	margin: 8px 0px 8px 0px;
	padding: 5px 0px 0px 0px;
	color: #618937;
	background: #FFFFFF;
	font: bold 14px Verdana, Arial, sans-serif;
}

#content h2.sponsors {
	margin: 20px 0px 0px 0px;
	padding: 5px;
	color: #000000;
	background: #C1D7AB;
	font-size: 14px;
}

#content h3 {
	margin: 20px 0px 5px 0px;
	padding: 5px 0px 0px 0px;
	color: #666666;
	background: #FFFFFF;
	font: bold 13px Verdana, Arial, sans-serif;
}

#content p {
	margin: 5px 0px 9px 0px;
	padding: 0px;
}

#content p.indent {
	margin-left: 35px;
}

#content p.quote {
	border: 1px dotted #999999;
	padding: 5px;
	background: #ECECEC;
	color: #333333;
}

#content ul {
	margin: 5px 10px 5px 30px;
	padding: 0px;
	list-style: circle;
}

#content ol {
	margin: 5px 10px 5px 30px;
	padding: 0px;
	list-style: decimal;
}

#content li {
	margin: 0px;
	padding: 0px;
	text-align: left;
}

#content img.person {
	border: 1px solid #000000;
	float: right;
	margin: 3px;
	width: 100px;
}

#content table {
	border: 0px;
}

#content th {
	padding: 2px;
	font: bold 11px Verdana, Arial, sans-serif;
	text-align: center;
}

#content td {
	padding: 3px;
	font: 11px Verdana, Arial, sans-serif;
	text-align: left;
	vertical-align: top;
}

#footer {
	clear: both;
	margin: 25px 5px 0px 5px;
	padding: 5px 10px 5px 10px;
	color: #666666;
	background: #E5E5E5;
	font: 10px Verdana, Arial, sans-serif;
	text-align: center;
	height: 15px;
}

.leftfootertext {
	float: left;
	margin: 0px;
	padding: 0px;
	display: inline;
	text-align: left;
}

.rightfootertext {
	float: right;
	margin: 0px;
	padding: 0px;
	display: inline;
	text-align: right;
}

#footer a:link, #footer a:visited {
	color: #666666;
	background: #E5E5E5;
	text-decoration: none;
	font-weight: normal;
}

#footer a:hover, #footer a:active {
	color: #41651A;
	background: #E5E5E5;
	text-decoration: underline;
	font-weight: normal;
}


.regbutton {
	border: solid 2px #FFFFFF;
	padding: 5px;
	background: #97C567;
	color: #97C567;
	font: 14px Arial, Verdana, sans-serif;
	text-align: center;
}
.regbutton2 {
	border: solid 1px #808080;
	padding: 0px;
	background: #FFFFFF;
	color: #97C567;
	font-size: 14px;
	text-align: center;
}
.rowline {
	border-top: 1px dotted #414141;
	text-align: center;
}
.strikethrough {
	text-decoration: line-through;
}
table.form {
	border: 1px dotted #414141;
	font-size: 14px;
}
table.regtable {
	border: 1px dotted #414141;
}
.earlybird {
	color: #FF0000;
	background: #FFF17F;
}
.fineprint {
	font-size: 9px;
	font-style: italic;
}


/*Styles for conference schedule table */

#schedule {
	margin: 0;
	padding: 0;
}

#schedule h2 {
	margin: 0px;
	padding: 5px 10px 5px 10px;
	color: white;
	font: 1.5 em Verdana, Arial, sans-serif;
	text-align: center;
	background: #888;
}

#schedule tr.dayheading {
	background: #888;
}


#schedule th {
	border: none;
	margin: 0px;
	padding: 3px;
}

#schedule .columnheading {
	background:#D4D4D4;
	font-size: 1em;
	padding: 5px;
}

#schedule td.time {
	background:#F4F4F4;
	text-align:center;
	font-weight:normal;
	border: none;
}

#schedule td.sponsor {
	background:#ECEDFF;
	text-align:center;
	padding: 150px 0px 0px 0px;
	margin: 0px;
	font-weight: bolder;
	border: none;
}

#schedule td{
	padding: 5px;
	margin: 0;
}

#schedule td.break{
	background:#FFFBC1;
	text-align: center;
	font-weight: normal;
	
}

#schedule .room404{
	background: #D4EAFF;
}

#schedule .room401{
	background: #ECF6FF;
}
#schedule .room402{
	background: #D4EAFF;
}

#schedule a.schedulelink, a.schedulelink:link, a.schedulelink:visited {
	color: #333333;
	text-decoration: none;
	font-weight: bold;
}
#schedule a.schedulelink:active, a.schedulelink:hover {
	color: #336699;
	text-decoration: underline;
	font-weight: bold;
}
